# Database (optional - app works with mock data if not configured)
MONGODB_URI="mongodb+srv://<username>:<password>@<cluster-url>/?appName=<app-name>"

# LLM Providers (at least one required for real AI, otherwise uses mock)
OPENAI_API_KEY="sk-proj-..."
GOOGLE_GENERATIVE_AI_API_KEY="AIza..."
ANTHROPIC_API_KEY="sk-ant-..."
DEFAULT_LLM_PROVIDER="google"

# Auth
NEXTAUTH_SECRET="your-nextauth-secret"
NEXTAUTH_URL="http://localhost:3000"

# Base Path (IMPORTANT: Set this in Vercel if deploying to subdirectory like /vector)
# NEXT_PUBLIC_BASE_PATH="/vector"

# Force mock mode (useful for testing without DB/LLM)
# VECTOR_USE_MOCK_ORCHESTRATOR="true"
